{
    "name": "intervention/validation",
    "description": "Additional Validator Functions for the Laravel Framework",
    "keywords": ["validation", "Validator", "laravel", "bic", "iban", "creditcard", "isbn", "isin"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Vogel",
            "email": "oliver@olivervogel.com",
            "homepage": "https://olivervogel.com/"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~4|~5|~6",
        "illuminate/validation": "~4|~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "php-coveralls/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Intervention\\Validation\\": "src/Intervention/Validation"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Intervention\\Validation\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Intervention\\Validation\\ValidationServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
